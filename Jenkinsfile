                echo 'Construyendo la aplicacion' 
            }
        }
        stage('Test') { 
            steps {
                echo 'Testeando la aplicacion'
            }
        }
        stage('Deploy') { 
            steps {
                echo 'Deployando la aplicacion' 
            }
        }
    }
}
